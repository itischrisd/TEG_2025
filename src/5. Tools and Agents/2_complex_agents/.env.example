# OpenAI API Configuration
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Optional: OpenAI Organization ID (if you belong to multiple organizations)
# OPENAI_ORG_ID=your_organization_id_here

# Optional: Custom OpenAI Base URL (for compatible services)
# OPENAI_BASE_URL=https://api.openai.com/v1

# LangSmith Configuration (Optional - for tracing and monitoring)
# Get your API key from: https://smith.langchain.com/
# LANGCHAIN_API_KEY=your_langsmith_api_key_here
# LANGCHAIN_TRACING_V2=true
# LANGCHAIN_PROJECT=complex-agents

# Development Settings
# Set to 'true' to enable debug logging
DEBUG=false

# Set to 'true' to enable verbose agent execution logs
AGENT_VERBOSE=false

# Model Configuration (Optional - defaults are provided in code)
# DEFAULT_MODEL=gpt-4o-mini
# DEFAULT_TEMPERATURE=0